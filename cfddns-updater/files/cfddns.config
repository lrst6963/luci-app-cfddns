config main 'config'
    option enabled '0'
    option email 'a@b.com'
    option api_key 'your_api_key_here'
    option zone_name 'example.com'
    option record_name 'sub.example.com'
    option record_type 'A'
    option ip_source 'network'
    option ip_service 'https://test.ipw.cn'
    option ip_interface 'wan'
    option ttl '300'
    option update_interval '60'
    option log_file '/var/log/cfddns.log'
